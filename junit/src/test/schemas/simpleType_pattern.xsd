	<xs:element name="Initials" type="InitialsType"/>
	
  	<xs:simpleType name="InitialsType">
    	<xs:restriction base="xs:string">
     		<xs:pattern value="[A-Z][A-Z][A-Z]"/>
    	</xs:restriction>
  	</xs:simpleType>
</xs:schema>